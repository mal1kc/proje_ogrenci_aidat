{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "File": {
      "Path": "app.log",
      "Append": true,
      "MinLevel": "Debug",
      "FileSizeLimitBytes": 1000000,
      "MaxRollingFiles": 3
    }
  },
  "AllowedHosts": "*",
  "SeedData": {
    "SeedDB": true,
    "VerboseLogging": true
  }
}
