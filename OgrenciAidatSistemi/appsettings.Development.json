{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Debug",
      "OgrenciAidatSistemi": "Debug",
      "OgrenciAidatSistemi.Models": "Debug",
      "OgrenciAidatSistemi.Controllers": "Debug"
    },
    "File": {
      "Path": "app.dev.log",
      "Append": true,
      "MinLevel": "Debug",
      "FileSizeLimitBytes": 1000000,
      "MaxRollingFiles": 3
    },
    "Console": {
      "IncludeScopes": "false"
    }
  },
  "AllowedHosts": "*",
  "SeedData": {
    "SeedDB": true,
    "VerboseLogging": true
  }
}