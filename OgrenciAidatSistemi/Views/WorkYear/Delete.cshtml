@model WorkYearView

@{
    ViewData["Title"] = "Delete Work Year";
}

<h3>Are you sure you want to delete this work year?</h3>

<div class="col-md-6">
    <partial name="_WorkYearDetailPartial" model="Model" />
</div>

<form asp-action="DeleteConfirmed" asp-route-id="@Model.Id" method="post">
    <input type="submit" value="Delete" class="btn btn-danger" />
    <a asp-action="List" class="btn btn-secondary">Back to List</a>
</form>
