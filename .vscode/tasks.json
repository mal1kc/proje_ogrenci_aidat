{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "dotnet",
			"task": "build ${workspaceFolder}/OgrenciAidatSistemi/OgrenciAidatSistemi.csproj",
			"file": "${workspaceFolder}/OgrenciAidatSistemi/OgrenciAidatSistemi.csproj",
			"group": "build",
			"problemMatcher": [],
			"label": "dotnet: build ${workspaceFolder}/OgrenciAidatSistemi/OgrenciAidatSistemi.csproj"
		},
		{
			"type": "shell",
			"label": "dotnet: watch ${workspaceFolder}/OgrenciAidatSistemi/OgrenciAidatSistemi.csproj",
			"command": "dotnet watch --project ${workspaceFolder}/OgrenciAidatSistemi/OgrenciAidatSistemi.csproj run",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": []
		},
		{
			"label": "Open External Terminal at Workspace",
			"type": "shell",
			"command": "${config:terminal.external.windowsExec}",
			"args": [
				"start",
				"--cwd",
				"'${workspaceFolder}'"
			],
			"problemMatcher": [],
			"group": {
				"kind": "test",
				"isDefault": true
			}
		}
	]
}
